# APP_NAME=LWPHP
# APP_ENV=development
# APP_DEBUG=true
# APP_URL=http://localhost:8000
# APP_TIMEZONE=UTC

# Database
# DB_CONNECTION=mysql
# DB_DRIVER=pdo_mysql
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=lwphp_db
# DB_USERNAME=root
# DB_PASSWORD=

# Cache
# CACHE_DRIVER=file

# # Redis (only needed when CACHE_DRIVER=redis)
# REDIS_URL=tcp://127.0.0.1:6379
# REDIS_TTL=3600

# # Memcached (only needed when CACHE_DRIVER=memcached)
# MEMCACHED_HOST=127.0.0.1
# MEMCACHED_PORT=11211

# # Session
# SESSION_DRIVER=file
# SESSION_LIFETIME=120
# SESSION_COOKIE=lwphp_session

# # Logging
# LOG_CHANNEL=app
# LOG_LEVEL=debug

# RATE_LIMIT_MAX=        # max requests per window
# RATE_LIMIT_WINDOW=     # window in seconds
# RATE_LIMIT_BURST=       # extra burst on top of max
# MAX_BODY_KB=         # max request body size in KB
# LWPHP_BLOCKED_IPS=
# LWPHP_ALLOWED_IPS=        # empty = open to all
